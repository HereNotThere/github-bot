{
  "name": "towns-github-bot",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "dev": "bun run --watch src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint --format unix ./src  --max-warnings=0",
    "lint:fix": "eslint --format unix ./src  --max-warnings=0 --fix",
    "start": "bun run src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@octokit/app": "^16.1.2",
    "@octokit/rest": "^22.0.1",
    "@octokit/webhooks": "^14.1.3",
    "@towns-protocol/bot": "^0.0.427",
    "@towns-protocol/proto": "^0.0.427",
    "@towns-protocol/web3": "^0.0.427",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.7",
    "picomatch": "^4.0.3",
    "postgres": "^3.4.7",
    "viem": "^2.40.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@prettier/plugin-oxc": "^0.1.2",
    "@types/bun": "^1.3.3",
    "@types/picomatch": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-unix": "^9.0.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tsdoc": "^0.5.0",
    "prettier": "^3.7.3",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.48.1"
  },
  "files": [
    "/dist"
  ]
}
